{
  "arrowParens": "always",
  "useTabs": false,
  "singleQuote": true,
  "tabWidth": 2,
  "semi": true,
  "trailingComma": "none",
  "importOrder": [
    "<THIRD_PARTY_MODULES>",
    "^@/core/(.*)",
    "^@/core/component(.*)",
    "^@/components/Layout/(.*)",
    "^@/components/screens/(.*)",
    "^@/components/ui/(.*)",
    "^@/styles/(.*)",
    "^@/config/(.*)",
    "^@/utils/(.*)",
    "^@/services/(.*)",
    "^@/store/(.*)",
    "^@/api/(.*)",
    "^.+\\.(template\\.html|module\\.scss)",
    "^../(.*)",
    "^./(.*)"
  ],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true
}
